    "test:performance": "npx tsx test-performance-metrics.ts",
    "test:teacher-student": "npx tsx test-teacher-student-gepa.ts",
    "test:vs-langchain": "npx tsx test-vs-langchain-langgraph.ts",
    "demo:full-system": "npx tsx demo-full-system-capabilities.ts",
    "test:per-domain": "npx tsx test-per-domain-gepa-iterations.ts",
    "test:smart-retrieval": "npx tsx test-smart-retrieval.ts",
    "test:local-embeddings": "npx tsx test-local-embeddings.ts",
    "test:comprehensive": "npx tsx test-comprehensive-everything.ts",
    "test:collaborative": "npx tsx test-collaborative-tools.ts",
    "test:multimodal": "npx tsx test-multimodal-features.ts",
    "test:gepa-retrieval": "npx tsx test-gepa-retrieval.ts",
    "test:doc-to-json": "npx tsx test-dspy-gepa-doc-to-json.ts",
    "test:requirements": "npx tsx test-requirement-tracking.ts",
    "test:stagnation": "npx tsx test-stagnation-detection.ts",
    "test:auto-tuning": "npx tsx test-complete-auto-tuning.ts",
    "test:statistical-proof": "npx tsx test-statistical-proof-system.ts",
    "test:integration": "npx tsx test-integration-verification.ts",
