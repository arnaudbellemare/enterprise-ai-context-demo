    "test:full-irt": "npx tsx test-full-system-irt.ts",
    "deploy:supabase": "./deploy-supabase.sh",
    "seed:db": "node scripts/seed-database.js",
    "benchmark": "npm run test:fluid && npm run test:analysis && npm run test:stats",
    "benchmark:real": "npm run test:full-irt",
