    "demo:feedback": "echo 'Open http://localhost:3000/feedback-demo in your browser'",
    "test:real-deal": "npx tsx test-integration-verification.ts",
    "test:ace": "npx tsx test-ace-framework.ts",
    "test:ace-benchmark": "npx tsx test-ace-benchmark.ts",
    "benchmark:ace": "npx tsx test-ace-benchmark.ts",
    "validate": "npm run test:overfitting"
