{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "installCommand": "cd frontend && npm install",
  "devCommand": "cd frontend && npm run dev",
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "frontend/app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "ignoreCommand": "git diff --quiet HEAD~1 HEAD -- frontend/",
  "github": {
    "silent": true
  }
}
